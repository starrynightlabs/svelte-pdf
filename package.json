{
  "name": "svelte-pdf",
  "version": "1.0.20",
  "description": "svelte-pdf provides a component for rendering PDF documents using PDF.js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte2tsx": "^0.6.21",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "dependencies": {
    "pdfjs-dist": "^3.10.111"
  },
  "exports": {
    ".": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vinodnimbalkar/svelte-pdf.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinodnimbalkar/svelte-pdf/issues"
  },
  "homepage": "https://github.com/vinodnimbalkar/svelte-pdf#readme",
  "keywords": [
    "pdf",
    "svelte-pdf",
    "pdfjs",
    "pdf-print",
    "pdf-zoom",
    "pdf-rotation",
    "pdf-navigation"
  ]
}
